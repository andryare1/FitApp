  "components": {
    "schemas": {
      "Exercise": {
        "required": [
          "description",
          "imageUrl",
          "muscleGroup",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "muscleGroup": {
            "$ref": "#/components/schemas/MuscleGroup"
          },
          "imageUrl": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "MuscleGroup": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      },
      "Training": {
        "required": [
          "name",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "trainingExercises": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TrainingExercise"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TrainingExercise": {
        "required": [
          "exerciseId",
          "reps",
          "sets",
          "trainingId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "trainingId": {
            "type": "integer",
            "format": "int32"
          },
          "training": {
            "$ref": "#/components/schemas/Training"
          },
          "exerciseId": {
            "type": "integer",
            "format": "int32"
          },
          "exercise": {
            "$ref": "#/components/schemas/Exercise"
          },
          "sets": {
            "type": "integer",
            "format": "int32"
          },
          "reps": {
            "type": "integer",
            "format": "int32"
          },
          "weight": {
            "type": "number",
            "format": "float"
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "orderIndex": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "UserLoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserRegisterDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}